# 環境設定
ENVIRONMENT=development

# サーバー設定
SERVER_PORT=8080
SERVER_READ_TIMEOUT=10
SERVER_WRITE_TIMEOUT=10
SERVER_IDLE_TIMEOUT=60

# データベース設定（Supabase Transaction Pooler）
DB_HOST=
DB_PORT=6543
DB_USER=
DB_PASSWORD=
DB_NAME=postgres
DB_SSL_MODE=require

# PgAdmin設定
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=admin

# JWT設定
JWT_SECRET=your-jwt-secret
JWT_EXPIRATION=24
JWT_REFRESH_EXPIRATION=168

# AWS Cognito設定
COGNITO_USER_POOL_ID=
COGNITO_CLIENT_ID=

# DynamoDB設定
DYNAMO_REGION=ap-northeast-1
DYNAMO_USER_CONFIG_TABLE=selfpomodoro_user_configs_dev
DYNAMO_ROUND_OPTIMIZATION_TABLE=selfpomodoro_round_optimization_logs_dev
DYNAMO_SESSION_OPTIMIZATION_TABLE=selfpomodoro_session_optimization_logs_dev

# SQS設定（新規追加）
SQS_ROUND_OPTIMIZATION_URL=
SQS_SESSION_OPTIMIZATION_URL=

AWS_REGION=ap-northeast-1

# ロギング設定
LOG_LEVEL=info
